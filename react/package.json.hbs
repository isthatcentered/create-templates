{
    "name": "{{ package_name }}",
    "description": "{{ package_description }}",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@isthatcentered/log":"latest",
		"@isthatcentered/opaque":"latest",
		"@testing-library/jest-dom":"latest",
		"@testing-library/react":"latest",
		"@testing-library/user-event":"latest",
		"@types/jest":"latest",
		"@types/node":"latest",
		"@types/react":"latest",
		"@types/react-dom":"latest",
		"husky":"latest",
		"pretty-quick":"latest",
		"react": "latest",
		"react-dom": "latest",
		"react-scripts": "latest",
		"typescript": "latest"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"prettier": {
		"arrowParens": "avoid",
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"prettier": "^1.19.1"
	}
}
